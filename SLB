parsers:
  - reg: 'slb$' #在订阅链接后加入#slb，更新后生效
    yaml:
      append-proxy-groups:
        - name: ⚖️ 负载均衡-散列
          type: load-balance
          url: 'http://www.google.com/generate_204'
          interval: 120
          strategy: consistent-hashing
        - name: ⚖️ 负载均衡-轮询
          type: load-balance
          url: 'http://www.google.com/generate_204'
          interval: 120
          strategy: round-robin
      commands:
        - proxy-groups.⚖️ 负载均衡-散列.proxies=[]proxyNames|^(?=.*(香港|日本|新加坡|狮城))^((?!IPLC|高级|标准).)*$
        - proxy-groups.0.proxies.0+⚖️ 负载均衡-散列
        - proxy-groups.⚖️ 负载均衡-轮询.proxies=[]proxyNames|^(?=.*(香港|日本|新加坡|狮城))^((?!IPLC|高级|标准).)*$
        - proxy-groups.0.proxies.0+⚖️ 负载均衡-轮询
