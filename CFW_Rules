#TUN模式
tun:
  enable: true
  stack: gvisor
  auto-route: true
  auto-detect-interface: true
  dns-hijack:
    - any:53
 #节点自动选择
    type: url-test
    url: http://www.qualcomm.cn/generate_204
    interval: 600
    
 #微博策略组
  - name: Weibo
    type: select
    proxies:
      - DIRECT
      - Proxies
